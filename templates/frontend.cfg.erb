frontend <%= name %>
<% if ip.class == Array -%>
<% ip.each do |ipargs| -%>
	bind	<%= ipargs["ip"] %>:<%= ipargs["port"] || port %> <%= ipargs["bind_opts"] %>
<% end -%>
<% else -%>
	bind	<%= ip %>:<%= port %> <% if bind_opts -%><%= bind_opts %><% end %>
<% end -%>
	mode <%= mode %>
<% options.each do |option| -%>
	option <%= option %>
<% end -%>
<% acls.each do |acl| -%>
	acl <%= acl %>
<% end -%>
<% redirects.each do |redirect| -%>
	redirect <%= redirect %>
<% end -%>
<% use_backends.each do |use_backend| -%>
	use_backend <%= use_backend %>
<% end -%>
<% options.each do |option| -%>
	option <%= option %>
<% end -%>
<% if default_backend -%>
	default_backend <%= default_backend %>
<% end -%>

