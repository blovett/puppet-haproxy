<% if ip.class == Array -%>
frontend <%= name %>
<% ip.each do |ipargs| -%>
	bind	<%= ipargs["ip"] %>:<%= ipargs["port"] %> <%= ipargs["bind_opts"] %>
<% end -%>
<% else -%>
frontend <%= name %> <%= ip %>:<%= port %>
<% end -%>
	mode <%= mode %>
<% options.each do |option| -%>
	option <%= option %>
<% end -%>
<% acls.each do |acl| -%>
	acl <%= acl %>
<% end -%>
<% redirects.each do |redirect| -%>
	redirect <%= redirect %>
<% end -%>
<% use_backends.each do |use_backend| -%>
	use_backend <%= use_backend %>
<% end -%>
<% options.each do |option| -%>
	option <%= option %>
<% end -%>
<% if default_backend -%>
	default_backend <%= default_backend %>
<% end -%>

